        <messages/>
        <name>Regex in Phone Lead</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
    if (isLoading || newValue === '') {
        return;
    }
    var reg = /^[0]\d{10}/;
    if (!reg.test(newValue)) {
        g_form.setValue('u_phone', '');
        g_form.showFieldMsg('u_phone', 'Phone number should start with 0 and be 10 digits', "error");
    }
}]]></script>
