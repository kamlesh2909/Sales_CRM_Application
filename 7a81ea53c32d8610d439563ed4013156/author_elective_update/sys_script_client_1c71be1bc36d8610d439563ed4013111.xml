        <messages/>
        <name>Display info msg lead</name>
        <order/>
        <script><![CDATA[function onSubmit() {
    var firstName = g_form.getValue('u_firstname');
    var lastName = g_form.getValue('u_lastname');

    if (firstName && lastName) {
        var message = "Lead " + firstName + "  " + lastName + " is created";
        g_form.addInfoMessage(message);

        // Remove the info message after 5 seconds
        setTimeout(function() {
            g_form.clearMessages(); // Clear all messages after 5 seconds
        }, 10000); // 5000 milliseconds = 5 seconds
    } else {
        g_form.addErrorMessage("Please fill out both First Name and Last Name fields.");
        return false; // Prevent form submission if fields are not filled
    }

    return true; // Allow form submission if all conditions are met
}]]></script>
