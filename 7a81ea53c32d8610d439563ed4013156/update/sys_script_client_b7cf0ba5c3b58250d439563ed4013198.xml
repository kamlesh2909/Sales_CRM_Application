        <messages/>
        <name>Regex for Country</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
    if (isLoading || newValue === '') {
        return;
    }
    var countryRegex = /^[a-zA-Z]+$/;
    if (!countryRegex.test(newValue)) {
        g_form.setValue('u_country', '');
      

        g_form.showFieldMsg('u_country', 'Country name should not contain numeric values ', "error");
    
    }


}]]></script>
